const tableHtml = tableElement.outerHTML;

    // Encode the HTML content
    const encodedTableHtml = encodeURIComponent(tableHtml);

    // Create the mailto link with the encoded HTML content as the body
    const mailtoLink = `mailto:?subject=Table%20in%20Email&body=${encodedTableHtml}`;

    // Open the mailto link
    window.location.href = mailtoLink;
