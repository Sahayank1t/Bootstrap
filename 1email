 // Constructing the email body
    let emailBody = "Email Body:\n\n";

    // Iterate over the array
    for (let i = 0; i < dynamicArray.length; i++) {
        // Add content of each element to the email body
        emailBody += "Element " + (i+1) + ":\n";
        for (let j = 0; j < dynamicArray[i].length; j++) {
            emailBody += dynamicArray[i][j] + "\n";
        }
        emailBody += "\n";
    }
